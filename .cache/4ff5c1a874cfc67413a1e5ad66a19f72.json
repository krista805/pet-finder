":16}},{"generated":{"line":54,"column":27},"source":"js/main.js","original":{"line":61,"column":8}},{"name":"div","generated":{"line":54,"column":28},"source":"js/main.js","original":{"line":61,"column":28}},{"generated":{"line":54,"column":31},"source":"js/main.js","original":{"line":61,"column":8}},{"generated":{"line":55,"column":0},"source":"js/main.js","original":{"line":62,"column":5}},{"generated":{"line":55,"column":5},"source":"js/main.js","original":{"line":39,"column":4}},{"generated":{"line":56,"column":0},"source":"js/main.js","original":{"line":63,"column":1}}],"sources":{"js/main.js":"import fetchJsonp from 'fetch-jsonp';\nimport { isValidZip, showAlert} from './validate';\n\nconst petForm = document.querySelector('#pet-form');\n\npetForm.addEventListener('submit', fetchAnimals);\n\n// Fetch Animals from API\nfunction fetchAnimals(e) {\n    e.preventDefault();\n\n    //Get User Input\n    const animal = document.querySelector('#animal').value;\n    const zip = document.querySelector('#zip').value;\n\n    //Validate Zip\n    if(!isValidZip(zip)) {\n        showAlert('Please Enter A Valid Zipcode', 'danger');\n        return;\n    }\n\n    //Fetch Pets\n    fetchJsonp(`http://api.petfinder.com/pet.find?format=json&key=c91a6a690ee530d76b6073bf65b446e6&animal=${animal}&location=${zip}&callback=callback`, {\n        jsonpCallbackFunction: 'callback'\n    })\n\n    .then(res => res.json())\n    .then(data => showAnimals(data.petfinder.pets.pet))\n    .catch(err => console.log(err));\n}\n\n//Show Listings of Pets\nfunction showAnimals(pets) {\n    const results = document.querySelector('#results');\n\n    //Clear First\n    results.innerHTML = '';\n    //Loop Through Pets\n    pets.forEach((pet) => {\n        console.log(pet);\n        const div = document.createElement('div');\n        div.classList.add('card', 'card-body', 'mb-3');\n        div.innerHTML = `\n            <div class=\"row\">\n                <div class=\"col-sm-6\">\n                    <h4>${pet.name.$t} (${pet.age.$t})</h4>\n                    <p class=\"text-secondary\">${pet.breeds.breed.$t}</p>\n                    <p>${pet.contact.address1.$t} ${pet.contact.city.$t} ${pet.contact.zip.$t}</p>\n                    <ul class=\"list-group\">\n                        <li class=\"list-group-item\">Phone: ${pet.contact.phone.$t}</li>\n                        ${pet.contact.email.$t ? `<li class=\"list-group-item\">Email: ${pet.contact.email.$t}</li>` : ``}\n                        <li class=\"list-group-item\">Shelter ID: ${pet.shelterId.$t}</li>\n                    </ul>\n                </div>\n                <div class=\"col-sm-6 text-center\">\n                    <img class=\"img-fluid rounded-circle mt-2\" src=\"${pet.media.photos.photo[3].$t}\">\n                </div>\n            </div>\n        `;\n\n        results.appendChild(div);\n    });\n}\n"},"lineCount":null}},"hash":"073010687f690e69ea9c31b5dd33475d","cacheData":{"env":{}}}